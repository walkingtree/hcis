<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="userManagementController" class="com.wtc.controller.UserManagementController">
		<property name="userManager" ref="userManagerService">
		</property>
	</bean>
	<bean id="roleManagementController" class="com.wtc.controller.RoleManagementController">
		<property name="roleManager" ref="roleManagerService"></property>
	</bean>
</beans>